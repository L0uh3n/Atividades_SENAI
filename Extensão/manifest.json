{
    "name": "Extensão Inova SENAI",
    "description": "Extensão para deficientes visuais realizarem compras e efetuar pagamentos online.",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["https://pt-br.facebook.com/"],
          "js": ["content.js"]
        }
      ],
    "browser_action": {
        "default_title": "Extensão Inova SENAI",
        "default_popup": "popup/popup.html"
    },
    "permissions": ["tabs", "storage"]
}